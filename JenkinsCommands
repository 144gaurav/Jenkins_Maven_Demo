java -jar jenkins.war

mvn archetype:generate -DgroupId=com.abc -DartifactId=DemoProject -DarchetypeArtifactId-maven-archetype-quickstart -Dinteractivemode=false

echo "# Jenkins_Maven_Demo" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/144gaurav/Jenkins_Maven_Demo.git
git push -u origin main

run ngrok
ngrok http -host-header=localhost 8080

